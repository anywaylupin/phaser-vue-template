{
  "name": "phaser-vue",
  "version": "0.0.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "vite",
    "editor": "phasereditor2d-launcher -project . -port 1959",
    "editor-remote": "phasereditor2d-launcher -project . -disable-open-browser -public -port 1959",
    "build": "run-p type-check \"build-only {@}\" --",
    "preview": "vite preview",
    "build-only": "vite build",
    "type-check": "vue-tsc --build --force",
    "lint": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --fix --ignore-path .gitignore",
    "format": "prettier --write src/",
    "fix": "pnpm lint && pnpm format"
  },
  "dependencies": {
    "phaser": "^3.60.0",
    "vue": "^3.3.11"
  },
  "devDependencies": {
    "@types/node": "^18.19.3",
    "@vitejs/plugin-vue": "^4.5.2",
    "@vitejs/plugin-vue-jsx": "^3.1.0",
    "@vue/eslint-config-prettier": "^8.0.0",
    "@vue/eslint-config-typescript": "^12.0.0",
    "eslint": "^8.49.0",
    "eslint-plugin-vue": "^9.17.0",
    "nuxtjs-phaser": "^3.0.17",
    "phaser3spectorjs": "^0.0.8",
    "phasereditor2d-launcher": "^3.32.0",
    "prettier": "^3.0.3",
    "typescript": "^5.0.4",
    "vite": "^5.0.10",
    "vue-tsc": "^1.8.25"
  }
}
